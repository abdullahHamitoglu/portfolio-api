{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "serverless": true
      }
    }
  ],
  "routes": [
    { "src": "/api-docs", "dest": "/public/swagger-static/index.html" },
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}
